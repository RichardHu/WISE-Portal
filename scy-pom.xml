    <build>
        <filters>
            <filter>src/main/resources/scy/scyfilter.properties</filter>
        </filters>
        <resources outputDirectory="${project.build.outputDirectory}">
            <resource>
                <filtering>true</filtering>
                <directory>src/main/resources</directory>

            </resource>
            <resource>
                <filtering>true</filtering>
                <directory>src/main/webapp/WEB-INF</directory>
            </resource>
            <resource>
                <!--targetPath>src/main/resources</targetPath-->
                <directory>src/main/resources/scy</directory>
                <includes>
                    <include>sendmail.properties</include>
                </includes>
                <filtering>true</filtering>
            </resource>
        </resources>
        <testResources
                testOutputDirectory="${project.build.outputDirectory}">
            <testResource>
                <filtering>true</filtering>
                <directory>src/test/resources</directory>
            </testResource>
        </testResources>
        <finalName>webapp</finalName>
        <plugins>
            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${basedir}/target/webapp/WEB-INF</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>src/main/resources/scy</directory>
                                    <filtering>true</filtering>
                                    <includes>
                                        <include>web.xml</include>
                                        <include>dispatcher-servlet.xml</include>
                                    </includes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <forkMode>once</forkMode>
                    <excludes>
                        <exclude>**/AllTests.java</exclude>
                    </excludes>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>unpack</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>

                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>eu.scy.client.desktop</groupId>
                                    <artifactId>scy-lab-webstart</artifactId>
                                    <version>9.8.0-SNAPSHOT</version>
                                    <type>jar</type>
                                    <type>jar</type>
                                    <outputDirectory>${project.build.directory}/${project.build.finalName}/webstart/
                                    </outputDirectory>
                                    <overWrite>true</overWrite>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>eu.scy</groupId>
                                    <artifactId>scy-planner-webstart</artifactId>
                                    <version>4.0.0-SNAPSHOT</version>
                                    <type>jar</type>
                                    <outputDirectory>${project.build.directory}/${project.build.finalName}/webstart/
                                    </outputDirectory>
                                    <overWrite>true</overWrite>
                                </artifactItem>
                                <!--artifactItem>
                                    <groupId>eu.scy.client.tools</groupId>
                                    <artifactId>scymapper-webstart</artifactId>
                                    <version>9.8.0-SNAPSHOT</version>
                                    <type>jar</type>
                                </artifactItem-->

                            </artifactItems>
                        </configuration>
                    </execution>

                    <execution>
                        <id>unpack-mission-content</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>eu.scy.content</groupId>
                                    <artifactId>scy-mission-one</artifactId>
                                    <version>${scy.version}</version>
                                    <type>jar</type>
                                    <outputDirectory>${project.build.directory}/${project.build.finalName}/content/
                                    </outputDirectory>
                                    <overWrite>true</overWrite>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>

                </executions>
            </plugin>

            <plugin>
                <artifactId>maven-war-plugin</artifactId>
                <configuration>
                    <webResources>
                        <resource>
                            <!-- this is relative to the pom.xml directory -->
                            <directory>
                                src/main/webapp/WEB-INF
                            </directory>
                            <!-- enable filtering -->
                            <filtering>true</filtering>
                            <includes>
                                <include>web.xml</include>
                                <include>dispatcher-servlet.xml</include>
                            </includes>
                            <!-- override the destination directory for this resource -->
                            <targetPath>WEB-INF</targetPath>
                        </resource>
                        <resource>
                            <directory>target/webapp/webstart/scyplanner/</directory>
                            <includes>
                                <include>scyplanner.jnlp</include>
                            </includes>
                            <filtering>true</filtering>
                            <targetPath>webstart</targetPath>
                        </resource>
                        <resource>
                            <directory>target/webapp/webstart/</directory>
                            <includes>
                                <include>scyplanner.jnlp</include>
                            </includes>
                            <targetPath>webstart/scyplanner</targetPath>
                        </resource>
                        <resource>
                            <directory>target/webapp/webstart/scylab/</directory>
                            <includes>
                                <include>scylab.jnlp</include>
                            </includes>
                            <filtering>true</filtering>
                            <targetPath>webstart</targetPath>
                        </resource>
                        <resource>
                            <directory>target/webapp/webstart/</directory>
                            <includes>
                                <include>scylab.jnlp</include>
                            </includes>
                            <targetPath>webstart/scylab</targetPath>
                        </resource>
                    </webResources>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>tomcat-maven-plugin</artifactId>
                <version>1.0-beta-1</version>
                <configuration>
                    <url>${deploy.url}</url>
                    <server>${deploy.server}</server>
                </configuration>
            </plugin>

            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <id>move-config</id>
                        <phase>process-classes</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/webapp/META-INF</outputDirectory>
                            <resources>
                                <resource>
                                    <includes>
                                        <include>context.xml</include>
                                    </includes>
                                    <directory>src/main/resources/scy</directory>
                                    <filtering>true</filtering>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

        </plugins>
    </build>
