                <OTNote id="ddcf2f30-5db1-11dd-8eba-010e12fd3f10" title="Plan and Predict" authorNotes="" hoverString="Pas Step" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false">
                  <jaxbXML>
                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="MasterWorkNote(96220)">
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_1" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_2" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_3" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_4" />
                      <itemBody>
                        <extendedTextInteraction hasInlineFeedback="false" placeholderText="" responseIdentifier="NOTE_RESPONSE_1" expectedLines="6">
                          <prompt>&lt;div style="font-size:1.2em"&gt;List one question you want to investigate using the molecular simulation.&lt;/div&gt;</prompt>
                        </extendedTextInteraction>
                        <extendedTextInteraction hasInlineFeedback="false" placeholderText="" responseIdentifier="NOTE_RESPONSE_2" expectedLines="36">
                          <prompt>&lt;div style="font-size:1.2em"&gt;What experiment would you do to answer your question? Write down three things for your experiment(s): (1) counter material (metal, glass or wood),(2) temperature of the cup and counter as either low, medium or high (the current model does not need you to enter the exact number for temperature), and (3) observation time.&lt;/div&gt;</prompt>
                        </extendedTextInteraction>
                        <extendedTextInteraction hasInlineFeedback="false" placeholderText="I predict that..." responseIdentifier="NOTE_RESPONSE_3" expectedLines="6">
