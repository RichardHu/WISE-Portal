                                        <OTText id="d09fe86c-5db4-11dd-a16a-e3339f59bf78" text="Beginning" />
                                        <object refid="d09f4d0f-5db4-11dd-a16a-e3339f59bf78" />
                                        <object refid="d09f9b40-5db4-11dd-a16a-e3339f59bf78" />
