                      <modalFeedback identifier="firstAttempt" showHide="show" outcomeIdentifier="FEEDBACK">Please read the information about reproductive cloning in the animation again.</modalFeedback>
                      <modalFeedback identifier="secondAttempt" showHide="show" outcomeIdentifier="FEEDBACK">Please read the information about reproductive cloning in the animation again.</modalFeedback>
                      <modalFeedback identifier="thirdAttempt" showHide="show" outcomeIdentifier="FEEDBACK">Please read the information about reproductive cloning in the animation again.</modalFeedback>
                      <modalFeedback identifier="fourthAttempt" showHide="show" outcomeIdentifier="FEEDBACK">Please read the information about reproductive cloning in the animation again.</modalFeedback>
                      <modalFeedback identifier="choice1" showHide="show" outcomeIdentifier="FEEDBACK">Yes, that is right!</modalFeedback>
                    </assessmentItem>
