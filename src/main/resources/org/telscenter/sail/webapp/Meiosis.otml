                <OTNote id="cb4b2a1a-5db4-11dd-a16a-e3339f59bf78" title="Note: Clones" authorNotes="" hoverString="Pas Step" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false">
                  <jaxbXML>
