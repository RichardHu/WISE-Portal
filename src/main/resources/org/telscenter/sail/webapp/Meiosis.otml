                <OTChallengeQuestion id="cb4b783c-5db4-11dd-a16a-e3339f59bf78" title="Check-Up: Cloning" authorNotes="" hoverString="Pas Step" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false" gotoStepIndex="2" maxAttempts="4">
                  <jaxbXML>
                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="ChallengeQuestion(4765)">
                      <responseDeclaration baseType="identifier" cardinality="single" identifier="PRIMARY_RESPONSE">
