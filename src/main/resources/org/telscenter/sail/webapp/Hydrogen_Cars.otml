        &lt;td colspan="2"&gt;&lt;div align="center"&gt;
                        &lt;span class="style4 style10"&gt;&lt;img src="http://wise.berkeley.edu/upload/23900/gasolineequ.gif" width="597" height="268"&gt;&lt;/span&gt;
                        
                  &lt;/TD&gt;
      &lt;tr&gt;
