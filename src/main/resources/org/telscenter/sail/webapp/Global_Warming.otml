                <OTNote id="66414ef7-5db6-11dd-ace9-3f6ab6d7651a" title="Thinking about the greenhouse effect." authorNotes="" hoverString="Pas Step" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false">
                  <jaxbXML>
                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="MasterWorkNote(96165)">
                      <responseDeclaration baseType="identifier" cardinality="single" identifier="NOTE_RESPONSE_1">
                        <correctResponse>
                          <value>choice</value>
                        </correctResponse>
                      </responseDeclaration>
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_2" />
                      <itemBody>
                        <choiceInteraction hasInlineFeedback="false" responseIdentifier="NOTE_RESPONSE_1" maxChoices="1" shuffle="false">
                          <prompt>Is the greenhouse effect good or bad for our Earth?</prompt>
                          <simpleChoice identifier="choice0">The greenhouse effect is good for the Earth.</simpleChoice>
