                <OTNote id="d1fc1c14-5d9a-11dd-a36e-47697d513d68" title="Generate some hypotheses" authorNotes="" hoverString="Pas Step" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false">
                  <hintList>
                    <OTPasHint text="Consider the relationship between a person's height and their POSITION from the steering wheel." />
                    <OTPasHint text="Consider the relationship between the car's speed before the crash and the driver's SPEED toward the steering wheel right after the crash." />
                    <OTPasHint text="Consider the relationship between the crumpling of the car and the DELAY TIME of the driver's motion." />
                  </hintList>
                  <jaxbXML>
                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="MasterWorkNote(96133)">
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_1" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_2" />
